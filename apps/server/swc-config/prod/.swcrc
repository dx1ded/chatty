{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "commonjs",
    "strict": true,
    "noInterop": false
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": false,
      "decorators": true
    },
    "target": "es2020",
    "transform": {
      "decoratorMetadata": true
    }
  },
  "minify": true,
  "sourceMaps": false
}
